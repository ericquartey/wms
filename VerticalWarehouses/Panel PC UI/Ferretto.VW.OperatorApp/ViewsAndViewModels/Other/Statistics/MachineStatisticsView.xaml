<UserControl x:Class="Ferretto.VW.OperatorApp.ViewsAndViewModels.Other.Statistics.MachineStatisticsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:local="clr-namespace:Ferretto.VW.OperatorApp"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:Resources="clr-namespace:Ferretto.VW.Resources;assembly=Ferretto.VW.Resources"
             xmlns:Controls="clr-namespace:Ferretto.VW.CustomControls.Controls;assembly=Ferretto.VW.CustomControls"
             xmlns:prism="http://prismlibrary.com/"
             mc:Ignorable="d" >

    <Grid   Width="1024" Height="608">
        <TextBlock  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,40,0,0" MaxWidth="500"
            Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
            Text="{x:Static Resources:OperatorApp.MachineStatisticsHeader}" />

        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,150,0,0" MaxWidth="300"
            ContentText="10.684"
            LabelText="{x:Static Resources:OperatorApp.MachineStatisticsSectionVerticalTotalCycles}"/>
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,425,0,0" MaxWidth="300"
            ContentText="6.136"
            LabelText="{x:Static Resources:OperatorApp.MachineStatisticsSectionShutterShutter2}"/>
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,205,0,0" MaxWidth="300"
            ContentText="39.562"
            LabelText="{x:Static Resources:OperatorApp.MachineStatisticsSectionVerticalTotalKm}"/>
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,370,0,0" MaxWidth="300"
            ContentText="3.235"
            LabelText="{x:Static Resources:OperatorApp.MachineStatisticsSectionShutterShutter1}"/>
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="560,150,0,0" MaxWidth="300"
            ContentText="7.235"
            LabelText="{x:Static Resources:OperatorApp.MachineStatisticsSectionOrizzontalTotalCycles}"/>
        <TextBlock  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,100,0,0" MaxWidth="500"
            Style="{StaticResource VWAPP_GeneralTextBlockStyle}"
            Text="{x:Static Resources:OperatorApp.MachineStatisticsSectionVertical}" />
        <TextBlock  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="560,100,0,0" MaxWidth="500"
            Style="{StaticResource VWAPP_GeneralTextBlockStyle}"
            Text="{x:Static Resources:OperatorApp.MachineStatisticsSectionOrizzontal}" />
        <TextBlock  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,320,0,0" MaxWidth="500"
            Style="{StaticResource VWAPP_GeneralTextBlockStyle}"
            Text="{x:Static Resources:OperatorApp.MachineStatisticsSectionShutters}" />
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,480,0,0" MaxWidth="300"
            ContentText="475"
            LabelText="{x:Static Resources:OperatorApp.MachineStatisticsSectionShutterShutter3}"/>
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="560,425,0,0" MaxWidth="300"
            ContentText="980"
            LabelText="{x:Static Resources:OperatorApp.MachineStatisticsSectionBaysBay2}"/>
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="560,370,0,0" MaxWidth="300"
            ContentText="2.235"
            LabelText="{x:Static Resources:OperatorApp.MachineStatisticsSectionBaysBay1}"/>
        <TextBlock  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="560,320,0,0" MaxWidth="500"
            Style="{StaticResource VWAPP_GeneralTextBlockStyle}"
            Text="{x:Static Resources:OperatorApp.MachineStatisticsSectionBays}" />
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="560,480,0,0" MaxWidth="300"
            ContentText="1.058"
            LabelText="{x:Static Resources:OperatorApp.MachineStatisticsSectionBaysBay3}"/>

    </Grid>
</UserControl>
