<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:custom="clr-namespace:Ferretto.VW.App.Controls.Controls"
                    xmlns:local="clr-namespace:Ferretto.VW.App.Controls.Styles">

    <Style x:Key="VWAPP_CustomMainWindowDiagnosticButton_TextBlockStyle"
           TargetType="{x:Type TextBlock}">
        <Setter Property="Foreground" Value="{DynamicResource VWAPP_CustomMainWindowDiagnosticButton_TextBlock_Foreground_Normal}" />
        <Setter Property="FontFamily" Value="{DynamicResource VWAPP_CustomMainWindowDiagnosticButton_TextBlock_FontFamily}" />
        <Setter Property="FontSize" Value="{DynamicResource VWAPP_CustomMainWindowDiagnosticButton_TextBlock_FontSize}" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
    </Style>
</ResourceDictionary>

