            d:DataContext="{d:DesignInstance Type=viewmodels:InstallationMenuViewModel,
                                             IsDesignTimeCreatable=False}"
            d:DesignHeight="700"
            d:DesignWidth="1024"
            prism:ViewModelLocator.AutoWireViewModel="True"
            mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <Grid Margin="15,0">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <views:InstallationNavigationMenuView Grid.Row="1" />
        </Grid>

        <Grid Grid.Column="1"
              Margin="15,0,15,0">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>

            <Grid Grid.Row="1">

                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <StackPanel Margin="5,0,5,0">
                    <TextBlock HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="{x:Static res:Menu.InstallationStatus}" />

                    <TextBlock Margin="0,0,0,0"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               FontFamily="{DynamicResource VWAPP_TitleFont}"
                               Style="{StaticResource VWAPP_ViewTitleSmallTextBlockStyle}">
                        <Run Text="Stato installazione incompleto. Eseguite" />
                        <Run Text="{Binding ProceduresCompleted}" /><Run Text="/" /><Run Text="{Binding ProceduresCount}" />
                        <Run Text="procedure" />
                    </TextBlock>

                    <Grid Margin="0,5,0,0"
                          HorizontalAlignment="Stretch"
                          VerticalAlignment="Stretch">

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>

                        <controls:PpcProgressBar Height="8"
                                                 HorizontalAlignment="Stretch"
                                                 VerticalAlignment="Center"
                                                 Color1="{DynamicResource FerrettoGreen}"
                                                 Color2="Green"
                                                 Maximum="100"
                                                 Minimum="0"
                                                 Value="{Binding ProceduresCompletedPercent}" />

                        <TextBlock Grid.Column="1"
                                   Margin="15,5,0,0"
                                   HorizontalAlignment="Right"
                                   VerticalAlignment="Center"
                                   FontFamily="{DynamicResource VWAPP_TitleFont}"
                                   Style="{StaticResource VWAPP_ViewTitleSmallTextBlockStyle}"
                                   Text="{Binding ProceduresCompletedPercent, StringFormat={}{0}%}" />
                    </Grid>

                </StackPanel>

