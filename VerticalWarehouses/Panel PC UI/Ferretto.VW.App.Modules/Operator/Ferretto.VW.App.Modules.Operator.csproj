<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <LanguageTargets>$(MSBuildToolsPath)\Microsoft.CSharp.targets</LanguageTargets>
    <TargetFramework>net471</TargetFramework>
    <OutputType>Library</OutputType>
  </PropertyGroup>

  <PropertyGroup>
    <OutputPath>..\..\Ferretto.VW.App\bin\$(Configuration)</OutputPath>
    <RunPostBuildEvent>Always</RunPostBuildEvent>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
    <Reference Include="System.Xaml" />
    <Reference Include="WindowsBase" />
  </ItemGroup>

   <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.SignalR.Client" Version="1.1.0" />
    <PackageReference Include="Prism.Core" Version="7.1.0.431" />
    <PackageReference Include="Prism.Unity" Version="7.1.0.431" />
    <PackageReference Include="Prism.Wpf" Version="7.1.0.431" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="4.6.0" />
    <PackageReference Include="Unity" Version="4.0.1" />
     <PackageReference Include="StyleCop.Analyzers" Version="1.1.118">
       <PrivateAssets>all</PrivateAssets>
       <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
     </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="**\*.xaml.cs">
      <DependentUpon>%(Filename)</DependentUpon>
      <SubType>Code</SubType>
    </Compile>
    <Page Include="**\*.xaml" Exclude="App.xaml">
      <Generator>MSBuild:Compile</Generator>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

  <ItemGroup>
    <None Remove="Views\DrawerOperations\DrawerWaitView - Copy (2).xaml" />
    <None Remove="Views\DrawerOperations\DrawerWaitView - Copy (3).xaml" />
    <None Remove="Views\DrawerOperations\DrawerWaitView - Copy (4).xaml" />
    <None Remove="Views\DrawerOperations\DrawerWaitView - Copy (5).xaml" />
    <None Remove="Views\DrawerOperations\DrawerWaitView - Copy (6).xaml" />
    <None Remove="Views\DrawerOperations\DrawerWaitView - Copy (7).xaml" />
    <None Remove="Views\DrawerOperations\DrawerWaitView - Copy.xaml" />
    <None Remove="Views\DrawerOperations\DrawerWaitView.xaml" />
    <None Remove="Views\EmptyView.xaml" />
    <None Remove="Views\ItemSearch\WaitingListDetailView.xaml" />
    <None Remove="Views\ItemSearch\WaitingListsView.xaml" />
    <None Remove="Views\OperatorMenuView.xaml" />
    <None Remove="Views\Others\DrawerCompactingDetailView.xaml" />
    <None Remove="Views\Others\DrawerCompactingView.xaml" />
    <None Remove="Views\Others\ImmediateDrawerCallView.xaml" />
    <None Remove="Views\Others\MaintenanceView.xaml" />
    <None Remove="Views\Others\MenuNavigationView.xaml" />
    <None Remove="Views\Others\OthersNavigationView.xaml" />
    <None Remove="Views\Others\Statistics\StatisticsCellsView.xaml" />
    <None Remove="Views\Others\Statistics\StatisticsDrawersView.xaml" />
    <None Remove="Views\Others\Statistics\StatisticsErrorsView.xaml" />
    <None Remove="Views\Others\Statistics\StatisticsMachineView.xaml" />
    <None Remove="Views\Others\Statistics\StatisticsMenuNavigationView.xaml" />
    <None Remove="Views\Others\Statistics\StatisticsNavigationView.xaml" />
    <None Remove="Views\Others\Statistics\StatisticsSpaceSaturationView.xaml" />
    <None Remove="Views\Others\Statistics\StatisticsWeightSaturationView.xaml" />
    <None Remove="Views\WaitingLists\ItemSearchDetailView.xaml" />
    <None Remove="Views\WaitingLists\ItemSearchMainView.xaml" />
    <None Remove="Views\WaitingLists\WaitingListsView - Copy.xaml" />
  </ItemGroup>

  <ItemGroup>
