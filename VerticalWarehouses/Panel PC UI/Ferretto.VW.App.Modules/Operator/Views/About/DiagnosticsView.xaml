                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>

                                    <Grid Grid.Column="0"
                                          Grid.ColumnSpan="3">

                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>


                                        <TextBlock Margin="0,10,0,0"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   Style="{DynamicResource VWAPP_CardSensor}"
                                                   Text="{res:Loc OperatorApp.ActiveMissionsTime}"
                                                   TextAlignment="Left" />

                                        <TextBlock Grid.Row="1"
                                                   Margin="0"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Top"
                                                   Style="{DynamicResource VWAPP_CardSensor_Data}">
                                            <Run Text="{Binding TotalStatistics.TotalInverterMissionTime}" />
                                            <Run Text="/" />
                                            <Run Text="{Binding TotalStatistics.TotalInverterPowerOnTime}" />
                                        </TextBlock>

                                        <Border Grid.RowSpan="2"
                                                Grid.Column="1"
                                                Margin="0,20"
                                                VerticalAlignment="Stretch"
                                                BorderBrush="{DynamicResource FerrettoMediumGray}"
                                                BorderThickness="1"
                                                Visibility="Hidden" />
                                    </Grid>

                                </Grid>
                            </Border>
                        </Border>
                    </Grid>

                    <Grid Grid.Column="1"
                          Margin="7,0,0,0">
                        <TextBlock Margin="0,10,0,0"
                                   Style="{StaticResource VWAPP_CustomLabelBig_LabelTextBlockStyle}"
                                   Text="{res:Loc OperatorApp.KmElevatorAxis}" />
                        <Border Margin="0,50,20,0"
                                VerticalAlignment="Top"
                                Background="{DynamicResource PpcBackground}">
                            <Border BorderBrush="{DynamicResource PpcBorderBrush}"
