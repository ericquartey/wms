    <ctrls:View.Resources>
        <!--#region Converters-->
        <convertes:BoolToBrushConverter x:Key="BoolToBrushConverter"
                                        FalseColor="{StaticResource PpcBackground}"
                                        TrueColor="{StaticResource FerrettoBlue}" />
        <!--#endregion-->

        <!--#region DataTemplate Body-->
        <DataTemplate DataType="{x:Type viewmodels:ErrorLoadunitMissingStepStart}">
            <Grid Margin="20"
                  DataContext="{Binding Path=DataContext, RelativeSource={RelativeSource AncestorType={x:Type ctrls:View}}}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>

                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>

                    <TextBlock FontSize="24"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Verifica cassetto:"
                               TextWrapping="Wrap" />

                    <StackPanel Grid.Column="1"
                                Margin="0,0,0,15"
                                HorizontalAlignment="Right"
                                Orientation="Vertical">
                        <TextBlock Margin="0"
                                   Style="{DynamicResource VWAPP_CustomLabel_LabelTextBlockStyle}"
                                   Text="{x:Static res:VWApp.ErrorCode}" />
                        <TextBlock Height="26"
                                   Background="{DynamicResource FerrettoRed}"
                                   Style="{DynamicResource VWAPP_CustomLabel_ContentTextBlockStyle}"
                                   Text="{Binding Error.Code, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static res:General.TargetNullValue}}"
                                   TextAlignment="Center"
                                   TextWrapping="Wrap" />
                    </StackPanel>

                </Grid>

                <Grid Grid.Row="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>

                    <controls:PpcLabeledText Grid.Column="0"
                                             Margin="0,0,15,0"
                                             HorizontalAlignment="Left"
                                             ContentText="{Binding Error.Id}"
                                             LabelText="Id" />

                    <controls:PpcLabeledText Grid.Column="1"
                                             Margin="0,0,15,0"
                                             ContentText="{Binding Error.Description}"
                                             LabelText="{x:Static res:VWApp.ErrorDescription}" />

                    <controls:PpcLabeledText Grid.Column="2"
                                             HorizontalAlignment="Right"
                                             ContentText="{Binding ErrorTime}"
                                             LabelText="{x:Static res:VWApp.FirstSeen}" />

                    <controls:PpcLabeledText Grid.Row="1"
                                             Grid.ColumnSpan="3"
                                             Margin="0,15,0,0"
                                             HorizontalAlignment="Left"
                                             ContentText="{Binding Error.Reason}"
                                             LabelText="{x:Static res:VWApp.ErrorReason}" />
                </Grid>

                <Image Grid.Row="2"
                       Margin="0,15,0,15"
                       HorizontalAlignment="Center"
                       VerticalAlignment="Center"
                       Source="{DynamicResource UnknownDrawer}"
                       Stretch="UniformToFill" />

                <controls:PpcButton Grid.Row="3"
                                    Command="{Binding MoveToNextCommand}"
                                    Content="Avanti"
                                    Style="{StaticResource WizardConfirmButton}" />
            </Grid>
        </DataTemplate>

        <DataTemplate DataType="{x:Type viewmodels:ErrorLoadunitMissingStepLoadunit}">
            <Grid Margin="20"
                  DataContext="{Binding Path=DataContext, RelativeSource={RelativeSource AncestorType={x:Type ctrls:View}}}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <StackPanel Grid.ColumnSpan="2">
                    <TextBlock FontSize="24"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Selezione del cassetto:"
                               TextWrapping="Wrap" />

                    <TextBlock FontSize="18"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="{Binding SubtitleStepLoadunit}"
                               TextWrapping="Wrap" />
                </StackPanel>

                <Border Grid.Row="1"
                        Background="{DynamicResource PpcBackground}">
                    <Border BorderBrush="{DynamicResource PpcBorderBrush}"
                            BorderThickness="{DynamicResource PpcBorderThickness}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>

                            <Grid Grid.ColumnSpan="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>

                                <TextBlock Margin="0,20,0,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontSize="{StaticResource FontSize_Normal}"
                                           FontWeight="Normal"
                                           Foreground="{DynamicResource FerrettoLightGray}"
                                           Text="{x:Static res:InstallationApp.CurrentCell}"
                                           TextAlignment="Left" />

                                <TextBlock Grid.Row="1"
                                           Margin="0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Top"
                                           FontSize="{StaticResource FontSize_Very_High}"
                                           Foreground="{DynamicResource PpcForeground}"
                                           Text="{Binding SelectedCell.Id}"
                                           TextWrapping="Wrap" />

                                <Border Grid.RowSpan="2"
                                        Grid.Column="1"
                                        Margin="0,25"
                                        VerticalAlignment="Stretch"
                                        BorderBrush="{DynamicResource FerrettoMediumGray}"
                                        BorderThickness="1" />

                            </Grid>

                            <Grid Grid.Column="3"
                                  Grid.ColumnSpan="3">

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>

                                <TextBlock Margin="0,20,0,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontSize="{StaticResource FontSize_Normal}"
                                           FontWeight="Normal"
                                           Foreground="{DynamicResource FerrettoLightGray}"
                                           Text="{x:Static res:InstallationApp.InitialPosition}"
                                           TextAlignment="Left" />

                                <TextBlock Grid.Row="1"
                                           Margin="0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Top"
                                           FontSize="{StaticResource FontSize_Very_High}"
                                           Foreground="{DynamicResource PpcForeground}"
                                           Text="{Binding SelectedCell.Position}"
                                           TextWrapping="Wrap" />

                                <Border Grid.RowSpan="2"
                                        Grid.Column="1"
                                        Margin="0,25"
                                        VerticalAlignment="Stretch"
                                        BorderBrush="{DynamicResource FerrettoMediumGray}"
                                        BorderThickness="1" />

                            </Grid>

                            <Grid Grid.Column="6"
                                  Grid.ColumnSpan="3">

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>

                                <TextBlock Margin="0,20,0,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontSize="{StaticResource FontSize_Normal}"
                                           FontWeight="Normal"
                                           Foreground="{DynamicResource FerrettoLightGray}"
                                           Text="{x:Static res:InstallationApp.CellSide}" />

                                <TextBlock Grid.Row="1"
                                           Margin="0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Top"
                                           FontSize="{StaticResource FontSize_Very_High}"
                                           Foreground="{DynamicResource PpcForeground}"
                                           Text="{Binding SelectedCell.Side, Converter={StaticResource EWSC}}"
                                           TextWrapping="Wrap" />

                                <Border Grid.RowSpan="2"
                                        Grid.Column="1"
                                        Margin="0,25"
                                        VerticalAlignment="Stretch"
                                        BorderBrush="{DynamicResource FerrettoMediumGray}"
                                        BorderThickness="1" />

                            </Grid>

                            <Grid Grid.Column="9"
                                  Grid.ColumnSpan="3"
                                  Margin="0,0,15,0">

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition />
                                </Grid.RowDefinitions>

                                <TextBlock Margin="0,20,0,0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontSize="{StaticResource FontSize_Normal}"
                                           FontWeight="Normal"
                                           Foreground="{DynamicResource FerrettoLightGray}"
                                           Text="{x:Static res:InstallationApp.Displacement}"
                                           TextAlignment="Left" />

                                <TextBlock Grid.Row="1"
                                           Margin="0"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Top"
                                           FontSize="{StaticResource FontSize_Very_High}"
                                           Foreground="{DynamicResource PpcForeground}"
                                           Text="{Binding Displacement, StringFormat={}{0:F1}, FallbackValue=0}"
                                           TextWrapping="Wrap" />

                            </Grid>

                        </Grid>
                    </Border>
                </Border>

                <Grid Grid.Row="2"
                      Margin="0,15,0,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>

                    <ctrls:PpcSpinEdit Grid.Row="2"
                                       Grid.Column="1"
                                       Width="200"
                                       Margin="0,0,15,0"
                                       HorizontalAlignment="Right"
                                       VerticalAlignment="Center"
                                       EditValue="{Binding InputLoadingUnitId, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                       IsEnabled="{Binding CanInputLoadingUnitId}"
                                       KeyboardCloseCommand="{Binding KeyboardCloseCommand}"
                                       KeyboardOpenCommand="{Binding KeyboardOpenCommand}"
                                       LabelText=""
                                       Mask="#0"
                                       MaxValue="999"
                                       MinValue="1" />

                    <controls:PpcButton Grid.Row="2"
                                        Grid.Column="2"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Center"
                                        Command="{Binding MoveLoadunitCommand}"
                                        Content="Rientra cassetto" />

                </Grid>

                <controls:PpcButton Grid.Row="3"
                                    Margin="0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Bottom"
                                    Command="{Binding MoveToNextCommand}"
                                    Content="Avanti"
                                    Style="{StaticResource WizardConfirmButton}" />
            </Grid>
        </DataTemplate>

        <DataTemplate DataType="{x:Type viewmodels:ErrorLoadunitMissingStepAutomaticMode}">
            <Grid Margin="20"
                  DataContext="{Binding Path=DataContext, RelativeSource={RelativeSource AncestorType={x:Type ctrls:View}}}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>

                <StackPanel Grid.ColumnSpan="2">
                    <TextBlock FontSize="24"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Passa in automatico:"
                               TextWrapping="Wrap" />

                    <TextBlock FontSize="18"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Terminata la procedura di identificazione cassetto, passa in autmatico."
                               TextWrapping="Wrap" />
                </StackPanel>

                <controls:PpcButton Grid.Row="3"
                                    Command="{Binding AutomaticCommand}"
                                    Content="Automatico"
                                    Style="{StaticResource WizardConfirmButton}" />
            </Grid>
        </DataTemplate>

        <!--#endregion-->
    </ctrls:View.Resources>

    <Grid Margin="15,0,0,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="60" />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition Width="Auto" />
        </Grid.ColumnDefinitions>
        <!--#region Title-->
        <StackPanel Grid.ColumnSpan="4"
                    Orientation="Horizontal">
            <TextBlock Grid.ColumnSpan="2"
                       Margin="0,10,15,10"
                       VerticalAlignment="Top"
                       Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                       Text="{Binding Error.Description}" />
        </StackPanel>
        <!--#endregion-->

        <!--#region Description-->
        <TextBlock Grid.Row="1"
                   Grid.ColumnSpan="4"
                   Height="Auto"
                   Margin="0,0,15,0"
                   VerticalAlignment="Top"
                   FontSize="{DynamicResource FontSize_Small}"
                   Style="{DynamicResource VWAPP_ViewTitleSmallTextBlockStyle}"
                   Text="{Binding Error.Reason}"
                   TextWrapping="Wrap" />
        <!--#endregion-->

        <!--#region Tails-->
        <controls:CardSensorAxisVertical Grid.Row="2"
                                         Grid.Column="3"
                                         Width="230"
                                         Height="90"
                                         MinHeight="90"
                                         Margin="0,15,15,0" />
        <controls:CardSensorAxisHorizontal Grid.Row="3"
                                           Grid.Column="3"
                                           Width="230"
                                           Height="90"
                                           MinHeight="90"
                                           Margin="0,15,15,0" />
        <controls:CardSensorShutter Grid.Row="4"
                                    Grid.Column="3"
                                    Width="230"
                                    Height="90"
                                    MinHeight="90"
                                    Margin="0,15,15,0" />
        <controls:CardSensorDrawer Grid.Row="5"
                                   Grid.Column="3"
                                   Width="230"
                                   Height="90"
                                   MinHeight="90"
                                   Margin="0,15,15,0" />

        <controls:PpcButton Grid.Row="6"
                            Grid.Column="3"
                            Margin="0,0,15,0"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Bottom"
                            Command="{Binding StopCommand}"
                            Content="{x:Static res:InstallationApp.Stop}" />
        <!--#endregion-->

        <!--#region Wizard-->
        <Grid Grid.Row="2"
              Grid.RowSpan="5"
              Grid.ColumnSpan="3"
              Margin="5,15,15,0">

            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="42" />
                <ColumnDefinition Width="17" />
                <ColumnDefinition Width="10" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition Height="1" />
                <RowDefinition Height="4*" />
                <RowDefinition Height="4*" />
                <RowDefinition Height="4*" />
                <RowDefinition Height="1" />
            </Grid.RowDefinitions>
            <!--#region Body-->
            <Border Grid.Row="0"
                    Grid.RowSpan="6"
                    Grid.Column="2"
                    Grid.ColumnSpan="2"
                    Margin="8,0,0,0"
                    Background="{StaticResource PpcBackground}"
                    BorderBrush="{StaticResource PpcBorderBrush}"
                    BorderThickness="{StaticResource PpcBorderThickness}"
                    CornerRadius="2,0,0,2">
                <ContentControl Content="{Binding CurrentStep, Mode=OneWay}" />
            </Border>
            <!--#endregion-->
            <!--#region TimeLine-->
            <Border Grid.Row="0"
                    Grid.RowSpan="6"
                    Style="{DynamicResource BorderTimeLine}" />

            <Grid Grid.Row="1">
                <Border Background="{Binding HasStepStart, Converter={StaticResource BoolToBrushConverter}, Mode=OneWay}"
                        Style="{DynamicResource BorderTimeLineCirle}" />
                <TextBlock Style="{DynamicResource BorderTimeLineCirleTextBlock}"
                           Text="1" />
            </Grid>
            <Grid Grid.Row="1"
                  Grid.Column="1"
                  Grid.ColumnSpan="2"
                  Visibility="{Binding HasStepStart, Converter={StaticResource BTVC}, Mode=OneWay}">
                <Path Height="24"
                      VerticalAlignment="Center"
                      StrokeThickness="0"
                      Style="{DynamicResource BorderTimeLinePath}" />
                <Path Style="{DynamicResource BorderTimeLinePath}" />
            </Grid>

            <Grid Grid.Row="2">
                <Border Background="{Binding HasStepLoadunit, Converter={StaticResource BoolToBrushConverter}, Mode=OneWay}"
                        Style="{DynamicResource BorderTimeLineCirle}" />
                <TextBlock Style="{DynamicResource BorderTimeLineCirleTextBlock}"
                           Text="2" />
            </Grid>
            <Grid Grid.Row="2"
                  Grid.Column="1"
                  Grid.ColumnSpan="2"
                  Visibility="{Binding HasStepLoadunit, Converter={StaticResource BTVC}, Mode=OneWay}">
                <Path Height="24"
                      VerticalAlignment="Center"
                      StrokeThickness="0"
                      Style="{DynamicResource BorderTimeLinePath}" />
                <Path Style="{DynamicResource BorderTimeLinePath}" />
            </Grid>

            <Grid Grid.Row="3">
                <Border Background="{Binding HasStepAutomaticMode, Converter={StaticResource BoolToBrushConverter}, Mode=OneWay}"
                        Style="{DynamicResource BorderTimeLineCirle}" />
                <TextBlock Style="{DynamicResource BorderTimeLineCirleTextBlock}"
                           Text="3" />
            </Grid>
            <Grid Grid.Row="3"
                  Grid.Column="1"
                  Grid.ColumnSpan="2"
                  Visibility="{Binding HasStepAutomaticMode, Converter={StaticResource BTVC}, Mode=OneWay}">
                <Path Height="24"
                      VerticalAlignment="Center"
                      StrokeThickness="0"
                      Style="{DynamicResource BorderTimeLinePath}" />
                <Path Style="{DynamicResource BorderTimeLinePath}" />
            </Grid>
            <!--#endregion-->
        </Grid>
        <!--#endregion-->
    </Grid>
</ctrls:View>
