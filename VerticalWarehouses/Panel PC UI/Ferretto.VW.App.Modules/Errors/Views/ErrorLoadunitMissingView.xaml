<ctrls:View x:Class="Ferretto.VW.App.Modules.Errors.Views.ErrorLoadunitMissingView"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            xmlns:controls="clr-namespace:Ferretto.VW.App.Controls.Controls;assembly=Ferretto.VW.App.Controls"
            xmlns:convertes="clr-namespace:Ferretto.VW.App.Controls.Converters;assembly=Ferretto.VW.App.Controls"
            xmlns:ctrls="clr-namespace:Ferretto.VW.App.Controls;assembly=Ferretto.VW.App.Controls"
            xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
            xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
            xmlns:prism="http://prismlibrary.com/"
            xmlns:res="clr-namespace:Ferretto.VW.App.Resources;assembly=Ferretto.VW.App.Resources"
            xmlns:viewmodels="clr-namespace:Ferretto.VW.App.Modules.Errors.ViewModels"
            d:DataContext="{d:DesignInstance Type=viewmodels:ErrorLoadunitMissingViewModel,
                                             IsDesignTimeCreatable=False}"
            d:DesignHeight="608"
            d:DesignWidth="774"
            prism:ViewModelLocator.AutoWireViewModel="True"
            mc:Ignorable="d">

    <ctrls:View.Resources>
        <!--#region Converters-->
        <convertes:BoolToBrushConverter x:Key="BoolToBrushConverter"
                                        FalseColor="{StaticResource PpcBackground}"
                                        TrueColor="{StaticResource FerrettoBlue}" />
        <!--#endregion-->

        <!--#region DataTemplate Body-->
        <DataTemplate DataType="{x:Type viewmodels:ErrorLoadunitMissingStepStart}">
            <Grid Margin="20"
                  DataContext="{Binding Path=DataContext, RelativeSource={RelativeSource AncestorType={x:Type ctrls:View}}}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>

                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>

                    <TextBlock FontSize="24"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Verifica cassetto:"
                               TextWrapping="Wrap" />

                    <StackPanel Grid.Column="1"
                                Margin="0,0,0,15"
                                HorizontalAlignment="Right"
                                Orientation="Vertical">
                        <TextBlock Margin="0"
                                   Style="{DynamicResource VWAPP_CustomLabel_LabelTextBlockStyle}"
                                   Text="{x:Static res:VWApp.ErrorCode}" />
                        <TextBlock Height="26"
                                   Background="{DynamicResource FerrettoRed}"
                                   Style="{DynamicResource VWAPP_CustomLabel_ContentTextBlockStyle}"
                                   Text="{Binding MachineError.Code, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, TargetNullValue={x:Static res:General.TargetNullValue}}"
                                   TextAlignment="Center"
                                   TextWrapping="Wrap" />
                    </StackPanel>

                </Grid>

                <Grid Grid.Row="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>

                    <controls:PpcLabeledText Grid.Column="0"
                                             Margin="0,0,15,0"
                                             HorizontalAlignment="Left"
                                             ContentText="{Binding MachineError.Id}"
                                             LabelText="Id" />

                    <controls:PpcLabeledText Grid.Column="1"
                                             Margin="0,0,15,0"
                                             ContentText="{Binding MachineError.Description}"
                                             LabelText="{x:Static res:VWApp.ErrorDescription}" />

                    <controls:PpcLabeledText Grid.Column="2"
                                             HorizontalAlignment="Right"
                                             ContentText="{Binding ErrorTime}"
                                             LabelText="{x:Static res:VWApp.FirstSeen}" />

                    <controls:PpcLabeledText Grid.Row="1"
                                             Grid.ColumnSpan="3"
                                             Margin="0,15,0,0"
                                             HorizontalAlignment="Left"
                                             ContentText="{Binding MachineError.Reason}"
                                             LabelText="{x:Static res:VWApp.ErrorReason}" />
                </Grid>

                <Image Grid.Row="2"
                       Margin="0,15,0,15"
                       HorizontalAlignment="Center"
                       VerticalAlignment="Center"
                       Source="{DynamicResource UnknownDrawer}"
                       Stretch="UniformToFill" />

                <controls:PpcButton Grid.Row="3"
                                    Command="{Binding MoveToNextCommand}"
                                    Content="Avanti"
                                    Style="{StaticResource WizardConfirmButton}" />
            </Grid>
        </DataTemplate>

        <DataTemplate DataType="{x:Type viewmodels:ErrorLoadunitMissingStepLoadunitOnElevator}">
            <Grid Margin="20"
                  DataContext="{Binding Path=DataContext, RelativeSource={RelativeSource AncestorType={x:Type ctrls:View}}}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <StackPanel Grid.ColumnSpan="2">
                    <TextBlock FontSize="24"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Selezione del cassetto:"
                               TextWrapping="Wrap" />

                    <TextBlock FontSize="18"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Selezionare l'id del cassetto presente sull'elevatore."
                               TextWrapping="Wrap" />
                </StackPanel>

                <ctrls:PpcSpinEdit Grid.Row="2"
                                   Grid.Column="1"
                                   Width="200"
                                   Margin="0,0,15,0"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   EditValue="{Binding LuIdOnElevator, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                   IsEnabled="{Binding CanLuIdOnElevator}"
                                   KeyboardCloseCommand="{Binding KeyboardCloseCommand}"
                                   KeyboardOpenCommand="{Binding KeyboardOpenCommand}"
                                   LabelText="Cassetto su elevatore"
                                   Mask="#0"
                                   MaxValue="999"
                                   MinValue="1" />

                <controls:PpcButton Grid.Row="3"
                                    Margin="0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Bottom"
                                    Command="{Binding MoveToNextCommand}"
                                    Content="Avanti"
                                    Style="{StaticResource WizardConfirmButton}" />
            </Grid>
        </DataTemplate>

        <DataTemplate DataType="{x:Type viewmodels:ErrorLoadunitMissingStepLoadunitOnBay1}">
            <Grid Margin="20"
                  DataContext="{Binding Path=DataContext, RelativeSource={RelativeSource AncestorType={x:Type ctrls:View}}}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <StackPanel Grid.ColumnSpan="2">
                    <TextBlock FontSize="24"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Selezione del cassetto (Baia 1):"
                               TextWrapping="Wrap" />

                    <TextBlock FontSize="18"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Selezionare l'id del/i cassetto/i presente/i in baia."
                               TextWrapping="Wrap" />
                </StackPanel>

                <Grid Grid.Row="2"
                      Margin="0,15,0,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>

                    <ctrls:PpcSpinEdit Grid.Row="2"
                                       Grid.Column="0"
                                       Width="200"
                                       Margin="0,0,15,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Center"
                                       EditValue="{Binding LuIdOnBay1Up, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                       KeyboardCloseCommand="{Binding KeyboardCloseCommand}"
                                       KeyboardOpenCommand="{Binding KeyboardOpenCommand}"
                                       LabelText="Postazione alta"
                                       Mask="#0"
                                       MaxValue="999"
                                       MinValue="1"
                                       Visibility="{Binding HasBay1PositionUpVisible, Converter={StaticResource BTVC}, Mode=OneWay}" />

                    <ctrls:PpcSpinEdit Grid.Row="2"
                                       Grid.Column="2"
                                       Width="200"
                                       Margin="15,0,0,0"
                                       HorizontalAlignment="Right"
                                       VerticalAlignment="Center"
                                       EditValue="{Binding LuIdOnBay1Down, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                       KeyboardCloseCommand="{Binding KeyboardCloseCommand}"
                                       KeyboardOpenCommand="{Binding KeyboardOpenCommand}"
                                       LabelText="Postazione bassa"
                                       Mask="#0"
                                       MaxValue="999"
                                       MinValue="1"
                                       Visibility="{Binding HasBay1PositionDownVisible, Converter={StaticResource BTVC}, Mode=OneWay}" />
                </Grid>

                <controls:PpcButton Grid.Row="3"
                                    Margin="0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Bottom"
                                    Command="{Binding MoveToNextCommand}"
                                    Content="Avanti"
                                    Style="{StaticResource WizardConfirmButton}" />
            </Grid>
        </DataTemplate>

        <DataTemplate DataType="{x:Type viewmodels:ErrorLoadunitMissingStepLoadunitOnBay2}">
            <Grid Margin="20"
                  DataContext="{Binding Path=DataContext, RelativeSource={RelativeSource AncestorType={x:Type ctrls:View}}}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <StackPanel Grid.ColumnSpan="2">
                    <TextBlock FontSize="24"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Selezione del cassetto (Baia 2):"
                               TextWrapping="Wrap" />

                    <TextBlock FontSize="18"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Selezionare l'id del/i cassetto/i presente/i in baia."
                               TextWrapping="Wrap" />
                </StackPanel>

                <Grid Grid.Row="2"
                      Margin="0,15,0,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>

                    <ctrls:PpcSpinEdit Grid.Row="2"
                                       Grid.Column="0"
                                       Width="200"
                                       Margin="0,0,15,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Center"
                                       EditValue="{Binding LuIdOnBay2Up, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                       KeyboardCloseCommand="{Binding KeyboardCloseCommand}"
                                       KeyboardOpenCommand="{Binding KeyboardOpenCommand}"
                                       LabelText="Postazione alta"
                                       Mask="#0"
                                       MaxValue="999"
                                       MinValue="1"
                                       Visibility="{Binding HasBay2PositionUpVisible, Converter={StaticResource BTVC}, Mode=OneWay}" />

                    <ctrls:PpcSpinEdit Grid.Row="2"
                                       Grid.Column="2"
                                       Width="200"
                                       Margin="15,0,0,0"
                                       HorizontalAlignment="Right"
                                       VerticalAlignment="Center"
                                       EditValue="{Binding LuIdOnBay2Down, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                       KeyboardCloseCommand="{Binding KeyboardCloseCommand}"
                                       KeyboardOpenCommand="{Binding KeyboardOpenCommand}"
                                       LabelText="Postazione bassa"
                                       Mask="#0"
                                       MaxValue="999"
                                       MinValue="1"
                                       Visibility="{Binding HasBay2PositionDownVisible, Converter={StaticResource BTVC}, Mode=OneWay}" />
                </Grid>

                <controls:PpcButton Grid.Row="3"
                                    Margin="0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Bottom"
                                    Command="{Binding MoveToNextCommand}"
                                    Content="Avanti"
                                    Style="{StaticResource WizardConfirmButton}" />
            </Grid>
        </DataTemplate>

        <DataTemplate DataType="{x:Type viewmodels:ErrorLoadunitMissingStepLoadunitOnBay3}">
            <Grid Margin="20"
                  DataContext="{Binding Path=DataContext, RelativeSource={RelativeSource AncestorType={x:Type ctrls:View}}}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <StackPanel Grid.ColumnSpan="2">
                    <TextBlock FontSize="24"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Selezione del cassetto (Baia 3):"
                               TextWrapping="Wrap" />

                    <TextBlock FontSize="18"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Selezionare l'id del/i cassetto/i presente/i in baia."
                               TextWrapping="Wrap" />
                </StackPanel>

                <Grid Grid.Row="2"
                      Margin="0,15,0,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>

                    <ctrls:PpcSpinEdit Grid.Row="2"
                                       Grid.Column="0"
                                       Width="200"
                                       Margin="0,0,15,0"
                                       HorizontalAlignment="Left"
                                       VerticalAlignment="Center"
                                       EditValue="{Binding LuIdOnBay3Up, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                       KeyboardCloseCommand="{Binding KeyboardCloseCommand}"
                                       KeyboardOpenCommand="{Binding KeyboardOpenCommand}"
                                       LabelText="Postazione alta"
                                       Mask="#0"
                                       MaxValue="999"
                                       MinValue="1"
                                       Visibility="{Binding HasBay3PositionUpVisible, Converter={StaticResource BTVC}, Mode=OneWay}" />

                    <ctrls:PpcSpinEdit Grid.Row="2"
                                       Grid.Column="2"
                                       Width="200"
                                       Margin="15,0,0,0"
                                       HorizontalAlignment="Right"
                                       VerticalAlignment="Center"
                                       EditValue="{Binding LuIdOnBay3Down, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                       KeyboardCloseCommand="{Binding KeyboardCloseCommand}"
                                       KeyboardOpenCommand="{Binding KeyboardOpenCommand}"
                                       LabelText="Postazione bassa"
                                       Mask="#0"
                                       MaxValue="999"
                                       MinValue="1"
                                       Visibility="{Binding HasBay3PositionDownVisible, Converter={StaticResource BTVC}, Mode=OneWay}" />
                </Grid>

                <controls:PpcButton Grid.Row="3"
                                    Margin="0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Bottom"
                                    Command="{Binding MoveToNextCommand}"
                                    Content="Avanti"
                                    Style="{StaticResource WizardConfirmButton}" />
            </Grid>
        </DataTemplate>

        <DataTemplate DataType="{x:Type viewmodels:ErrorLoadunitMissingStepAutomaticMode}">
            <Grid Margin="20"
                  DataContext="{Binding Path=DataContext, RelativeSource={RelativeSource AncestorType={x:Type ctrls:View}}}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>

                <StackPanel Grid.ColumnSpan="2">
                    <TextBlock FontSize="24"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Passa in automatico:"
                               TextWrapping="Wrap" />

                    <TextBlock FontSize="18"
                               Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                               Text="Terminata la procedura di identificazione cassetto, passa in autmatico."
                               TextWrapping="Wrap" />
                </StackPanel>

                <!--<controls:PpcButton Grid.Row="1"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Bottom"
                                    Command="{Binding MarkAsResolvedCommand}"
                                    Content="{x:Static res:VWApp.MarkAsResolved}" />-->

                <controls:PpcButton Grid.Row="3"
                                    Command="{Binding AutomaticCommand}"
                                    Content="Automatico"
                                    Style="{StaticResource WizardConfirmButton}" />
            </Grid>
        </DataTemplate>

        <!--#endregion-->
    </ctrls:View.Resources>

    <Grid Margin="15,0,0,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="60" />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition Width="Auto" />
        </Grid.ColumnDefinitions>
        <!--#region Title-->
        <StackPanel Grid.ColumnSpan="4"
                    Orientation="Horizontal">
            <TextBlock Grid.ColumnSpan="2"
                       Margin="0,10,15,10"
                       VerticalAlignment="Top"
                       Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                       Text="{Binding MachineError.Description}" />
        </StackPanel>
        <!--#endregion-->

        <!--#region Description-->
        <TextBlock Grid.Row="1"
                   Grid.ColumnSpan="4"
                   Height="Auto"
                   Margin="0,0,15,0"
                   VerticalAlignment="Top"
                   FontSize="{DynamicResource FontSize_Small}"
                   Style="{DynamicResource VWAPP_ViewTitleSmallTextBlockStyle}"
                   Text="{Binding MachineError.Reason}"
                   TextWrapping="Wrap" />
        <!--#endregion-->

        <!--#region Tails-->
        <controls:CardSensorAxisVertical Grid.Row="2"
                                         Grid.Column="3"
                                         Width="230"
                                         Height="90"
                                         MinHeight="90"
                                         Margin="0,15,15,0" />
        <controls:CardSensorAxisHorizontal Grid.Row="3"
                                           Grid.Column="3"
                                           Width="230"
                                           Height="90"
                                           MinHeight="90"
                                           Margin="0,15,15,0" />
        <controls:CardSensorShutter Grid.Row="4"
                                    Grid.Column="3"
                                    Width="230"
                                    Height="90"
                                    MinHeight="90"
                                    Margin="0,15,15,0" />
        <controls:CardSensorDrawer Grid.Row="5"
                                   Grid.Column="3"
                                   Width="230"
                                   Height="90"
                                   MinHeight="90"
                                   Margin="0,15,15,0" />

        <controls:PpcButton Grid.Row="6"
                            Grid.Column="3"
                            Margin="0,0,15,0"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Bottom"
                            Command="{Binding StopCommand}"
                            Content="{x:Static res:InstallationApp.Stop}" />
        <!--#endregion-->

        <!--#region Wizard-->
        <Grid Grid.Row="2"
              Grid.RowSpan="5"
              Grid.ColumnSpan="3"
              Margin="5,15,15,0">

            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="42" />
                <ColumnDefinition Width="17" />
                <ColumnDefinition Width="10" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition Height="1" />
                <RowDefinition Height="4*" />
                <RowDefinition Height="4*" />
                <RowDefinition Height="4*" />
                <RowDefinition Height="4*" />
                <RowDefinition Height="4*" />
                <RowDefinition Height="4*" />
                <RowDefinition Height="1" />
            </Grid.RowDefinitions>
            <!--#region Body-->
            <Border Grid.Row="0"
                    Grid.RowSpan="7"
                    Grid.Column="2"
                    Grid.ColumnSpan="2"
                    Margin="8,0,0,0"
                    Background="{StaticResource PpcBackground}"
                    BorderBrush="{StaticResource PpcBorderBrush}"
                    BorderThickness="{StaticResource PpcBorderThickness}"
                    CornerRadius="2,0,0,2">
                <ContentControl Content="{Binding CurrentStep, Mode=OneWay}" />
            </Border>
            <!--#endregion-->
            <!--#region TimeLine-->
            <Border Grid.Row="0"
                    Grid.RowSpan="7"
                    Style="{DynamicResource BorderTimeLine}" />

            <Grid Grid.Row="1">
                <Border Background="{Binding HasStepStart, Converter={StaticResource BoolToBrushConverter}, Mode=OneWay}"
                        Style="{DynamicResource BorderTimeLineCirle}" />
                <TextBlock Style="{DynamicResource BorderTimeLineCirleTextBlock}"
                           Text="1" />
            </Grid>
            <Grid Grid.Row="1"
                  Grid.Column="1"
                  Grid.ColumnSpan="2"
                  Visibility="{Binding HasStepStart, Converter={StaticResource BTVC}, Mode=OneWay}">
                <Path Height="24"
                      VerticalAlignment="Center"
                      StrokeThickness="0"
                      Style="{DynamicResource BorderTimeLinePath}" />
                <Path Style="{DynamicResource BorderTimeLinePath}" />
            </Grid>

            <Grid Grid.Row="2">
                <Border Background="{Binding HasStepLoadunitOnElevator, Converter={StaticResource BoolToBrushConverter}, Mode=OneWay}"
                        Style="{DynamicResource BorderTimeLineCirle}" />
                <TextBlock Style="{DynamicResource BorderTimeLineCirleTextBlock}"
                           Text="2" />
            </Grid>
            <Grid Grid.Row="2"
                  Grid.Column="1"
                  Grid.ColumnSpan="2"
                  Visibility="{Binding HasStepLoadunitOnElevator, Converter={StaticResource BTVC}, Mode=OneWay}">
                <Path Height="24"
                      VerticalAlignment="Center"
                      StrokeThickness="0"
                      Style="{DynamicResource BorderTimeLinePath}" />
                <Path Style="{DynamicResource BorderTimeLinePath}" />
            </Grid>

            <Grid Grid.Row="3">
                <Border Background="{Binding HasStepLoadunitOnBay1, Converter={StaticResource BoolToBrushConverter}, Mode=OneWay}"
                        Style="{DynamicResource BorderTimeLineCirle}" />
                <TextBlock Style="{DynamicResource BorderTimeLineCirleTextBlock}"
                           Text="3" />
            </Grid>
            <Grid Grid.Row="3"
                  Grid.Column="1"
                  Grid.ColumnSpan="2"
                  Visibility="{Binding HasStepLoadunitOnBay1, Converter={StaticResource BTVC}, Mode=OneWay}">
                <Path Height="24"
                      VerticalAlignment="Center"
                      StrokeThickness="0"
                      Style="{DynamicResource BorderTimeLinePath}" />
                <Path Style="{DynamicResource BorderTimeLinePath}" />
            </Grid>

            <Grid Grid.Row="4">
                <Border Background="{Binding HasStepLoadunitOnBay2, Converter={StaticResource BoolToBrushConverter}, Mode=OneWay}"
                        Style="{DynamicResource BorderTimeLineCirle}" />
                <TextBlock Style="{DynamicResource BorderTimeLineCirleTextBlock}"
                           Text="4" />
            </Grid>
            <Grid Grid.Row="4"
                  Grid.Column="1"
                  Grid.ColumnSpan="2"
                  Visibility="{Binding HasStepLoadunitOnBay2, Converter={StaticResource BTVC}, Mode=OneWay}">
                <Path Height="24"
                      VerticalAlignment="Center"
                      StrokeThickness="0"
                      Style="{DynamicResource BorderTimeLinePath}" />
                <Path Style="{DynamicResource BorderTimeLinePath}" />
            </Grid>

            <Grid Grid.Row="5">
                <Border Background="{Binding HasStepLoadunitOnBay3, Converter={StaticResource BoolToBrushConverter}, Mode=OneWay}"
                        Style="{DynamicResource BorderTimeLineCirle}" />
                <TextBlock Style="{DynamicResource BorderTimeLineCirleTextBlock}"
                           Text="5" />
            </Grid>
            <Grid Grid.Row="5"
                  Grid.Column="1"
                  Grid.ColumnSpan="2"
                  Visibility="{Binding HasStepLoadunitOnBay3, Converter={StaticResource BTVC}, Mode=OneWay}">
                <Path Height="24"
                      VerticalAlignment="Center"
                      StrokeThickness="0"
                      Style="{DynamicResource BorderTimeLinePath}" />
                <Path Style="{DynamicResource BorderTimeLinePath}" />
            </Grid>

            <Grid Grid.Row="6">
                <Border Background="{Binding HasStepAutomaticMode, Converter={StaticResource BoolToBrushConverter}, Mode=OneWay}"
                        Style="{DynamicResource BorderTimeLineCirle}" />
                <TextBlock Style="{DynamicResource BorderTimeLineCirleTextBlock}"
                           Text="6" />
            </Grid>
            <Grid Grid.Row="6"
                  Grid.Column="1"
                  Grid.ColumnSpan="2"
                  Visibility="{Binding HasStepAutomaticMode, Converter={StaticResource BTVC}, Mode=OneWay}">
                <Path Height="24"
                      VerticalAlignment="Center"
                      StrokeThickness="0"
                      Style="{DynamicResource BorderTimeLinePath}" />
                <Path Style="{DynamicResource BorderTimeLinePath}" />
            </Grid>
            <!--#endregion-->
        </Grid>
        <!--#endregion-->
    </Grid>
</ctrls:View>
