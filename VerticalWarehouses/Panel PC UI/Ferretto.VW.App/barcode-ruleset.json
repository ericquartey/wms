{
  "Contexts": [
    {
      "Name": "ItemsSearch",
      "Rules": [
        {
          "Name": "Filter items by bar code",
          "Patterns": [ "/ITM(?<item_code>[0-9A-Za-z]{4,7})/" ],
          "UserAction": "FilterItems"
        }
      ]
    },
    {
      "Name": "WaitingLists",
      "Rules": [
        {
          "Name": "Execute specified list",
          "Patterns": [ "/LST(?<item_code>[0-9A-Za-z]{4,7})/" ],
          "UserAction": "ExecuteList"
        }
      ]
    },
    {
      "Name": "PickItem",
      "Rules": [
        {
          "Name": "Pick item with given quantity",
          "Patterns": [ "/P(?<item_quantity>[0-9]{4})(?<item_code>[0-9A-Za-z]{4,7})/" ],
          "UserAction": "PickItem"
        }
      ]
    }
  ]
}
