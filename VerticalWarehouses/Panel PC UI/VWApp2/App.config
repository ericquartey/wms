<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <configSections>
    <section name="modules" type="Prism.Modularity.ModulesConfigurationSection, Prism.Wpf"/>
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.1" />
  </startup>

  <appSettings>
    <add key="WMS:DataService:Url" value="http://server_rd.ferrettogroup.com:10000" />
    <add key="WMS:DataService:Hubs:Data:Path" value="http://server_rd.ferrettogroup.com:10000/hubs/data" />
    <add key="AutomationService:Url" value="http://localhost:5000" />
    <add key="AutomationService:HealthChecks:Live:Path" value="/health/live" />
    <add key="AutomationService:HealthChecks:Ready:Path" value="/health/ready" />
    <add key="AutomationService:Hubs:Installation:Path" value="/installation-endpoint" />
    <add key="AutomationService:Hubs:Operator:Path" value="/operator-endpoint" />
  </appSettings>
  <modules>
      <module assemblyFile="Ferretto.VW.App.Modules.Layout.dll"
          moduleType="Ferretto.VW.App.Modules.Layout.LayoutModule, Ferretto.VW.App.Modules.Layout, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"
          moduleName="Layout"
          startupLoaded="false" >
      </module>
      <module assemblyFile="Ferretto.VW.App.Modules.Login.dll"
          moduleType="Ferretto.VW.App.Modules.Login.LoginModule, Ferretto.VW.App.Modules.Login, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"
          moduleName="Login"
          startupLoaded="false" >
    </module>
    <module assemblyFile="Ferretto.VW.App.Installation.dll"
          moduleType="Ferretto.VW.App.Installation.InstallationAppModule, Ferretto.VW.App.Installation, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"
          moduleName="Installation"
          startupLoaded="false">
    </module>
    <module assemblyFile="Ferretto.VW.App.Operator.dll"
          moduleType="Ferretto.VW.App.Operator.OperatorAppModule, Ferretto.VW.App.Operator, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"
          moduleName="Operator"
          startupLoaded="false">
    </module>
  </modules>
</configuration>
