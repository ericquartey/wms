<UserControl x:Class="Ferretto.VW.App.Modules.Operator.ViewsAndViewModels.Other.DrawerCompactingDetailView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:Resources="clr-namespace:Ferretto.VW.App.Resources;assembly=Ferretto.VW.App.Resources"
             xmlns:Controls="clr-namespace:Ferretto.VW.App.Controls.Controls;assembly=Ferretto.VW.App.Controls"
             mc:Ignorable="d" >
    <Grid  Width="1024" Height="608">
        <TextBlock  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,40,0,0" MaxWidth="500"
            Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
            Text="{x:Static Resources:OperatorApp.DrawerCompactingDetailHeader}" />

        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,200,0,0" MaxWidth="300"
            ContentText="201"
            LabelText="{x:Static Resources:OperatorApp.DrawerCompactingDetailSourceCell}"/>

        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,260,0,0" MaxWidth="300"
            ContentText="135"
            LabelText="{x:Static Resources:OperatorApp.DrawerCompactingDetailDestinationCell}"/>
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,140,0,0" MaxWidth="300"
            ContentText="1008"
            LabelText="{x:Static Resources:OperatorApp.DrawerCompactingDetailDrawer}"/>
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,360,0,0" MaxWidth="300"
            ContentText="Spostamento a sorgente"
            LabelText="{x:Static Resources:OperatorApp.DrawerCompactingDetailActualStep}"/>
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,420,0,0" MaxWidth="300"
            ContentText="4352,5"
            LabelText="{x:Static Resources:OperatorApp.DrawerCompactingDetailVerticalPosition}"/>
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="250,480,0,0" MaxWidth="300"
            ContentText="15789,4"
            LabelText="{x:Static Resources:OperatorApp.DrawerCompactingDetailOrizzontalPosition}"/>
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="580,200,0,0" MaxWidth="300"
            ContentText="250"
            LabelText="{x:Static Resources:OperatorApp.DrawerCompactingDetailRecoveredSpace}"/>
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="580,260,0,0" MaxWidth="300"
            ContentText="350"
            LabelText="{x:Static Resources:OperatorApp.DrawerCompactingDetailSpaceToRecover}"/>
        <Controls:CustomLabelTextBlockControl  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="580,140,0,0" MaxWidth="300"
            ContentText="4"
            LabelText="{x:Static Resources:OperatorApp.DrawerCompactingDetailRemainingFragment}"/>

    </Grid>
</UserControl>
