<?xml version="1.0" encoding="utf-8" ?>
<configuration>

<configSections>

   <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>

 </configSections>  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">    <targets>

     <target name="logfile" xsi:type="File" fileName="UICHATest.${longdate:cached=true}.log" archiveFileName="UICHATest.${shortdate}.{#}.log" archiveAboveSize="5242880" archiveEvery="Day" archiveNumbering="Rolling" maxArchiveFiles="20"/>

     <!-- <target name="logconsole" xsi:type="Console" /> -->

   </targets>    <rules>

     <!-- <logger name="*" minlevel="Info" writeTo="logconsole" /> -->

     <logger name="*" minlevel="Debug" writeTo="logfile"/>

   </rules>

 </nlog>

    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.1" />
    </startup>
</configuration>
