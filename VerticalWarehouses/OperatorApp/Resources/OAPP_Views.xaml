<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Ferretto.VW.OperatorApp.Resources"
                    xmlns:singleViews="clr-namespace:Ferretto.VW.OperatorApp.ViewsAndViewModels.SingleViews"
                    xmlns:views="clr-namespace:Ferretto.VW.OperatorApp.ViewsAndViewModels">
    <DataTemplate DataType="{x:Type views:NavigationButtonViewModel}">
        <views:NavigationButtonView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type singleViews:ArticlesViewModel}">
        <singleViews:ArticlesView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type singleViews:ListsViewModel}">
        <singleViews:ListsView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type singleViews:PickingViewModel}">
        <singleViews:PickingView />
    </DataTemplate>
</ResourceDictionary>
