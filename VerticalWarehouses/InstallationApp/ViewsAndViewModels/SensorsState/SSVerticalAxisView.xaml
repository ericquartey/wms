<local:BaseView x:Class="Ferretto.VW.InstallationApp.SSVerticalAxisView"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:custom="clr-namespace:Ferretto.VW.CustomControls.Controls;assembly=Ferretto.VW.CustomControls"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:local="clr-namespace:Ferretto.VW.InstallationApp"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:myType="clr-namespace:Ferretto.VW.Utils.Source;assembly=Ferretto.VW.Utils"
                 xmlns:prism="http://prismlibrary.com/"
                 xmlns:res="clr-namespace:Ferretto.VW.Resources;assembly=Ferretto.VW.Resources"
                 prism:ViewModelLocator.AutoWireViewModel="True"
                 mc:Ignorable="d">
    <Grid Width="774"
                 Height="608">
        <TextBlock Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}"
                       Text="{x:Static res:InstallationApp.VerticalAxis}" Margin="162,10,0,562" />
        <custom:CustomSensorControl Margin="162,367,338,188"
                                                LabelText="{x:Static res:InstallationApp.ZeroVerticalSensor}"
                                                SensorState="{Binding ZeroVerticalSensor, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                Style="{StaticResource VWAPP_CustomSensorControlStyle}" />
            <custom:CustomSensorControl Margin="162,270,338,285"
                                                LabelText="{x:Static res:InstallationApp.EmergencyEndRun}"
                                                SensorState="{Binding EmergencyEndRun, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                Style="{StaticResource VWAPP_CustomSensorControlStyle}" />
        <custom:CustomSensorControl Margin="162,162,338,393"
                                                LabelText="{x:Static res:InstallationApp.BrakeResistanceOvertemperature}"
                                                SensorState="{Binding BrakeResistanceOvertemperature, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                Style="{StaticResource VWAPP_CustomSensorControlStyle}" />
    </Grid>
</local:BaseView>
