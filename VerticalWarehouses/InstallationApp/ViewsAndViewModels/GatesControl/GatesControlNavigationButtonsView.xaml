<UserControl x:Class="Ferretto.VW.InstallationApp.ViewsAndViewModels.GatesControl.GatesControlNavigationButtonsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Ferretto.VW.InstallationApp.ViewsAndViewModels.GatesControl"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:res="clr-namespace:Ferretto.Common.Resources;assembly=Ferretto.Common.Resources"
             Height="610"
             mc:Ignorable="d">
    <Grid Height="610">
        <Grid.RowDefinitions>
            <RowDefinition Height="20" />
            <RowDefinition Height="510" />
            <RowDefinition Height="80" />
        </Grid.RowDefinitions>
        <StackPanel Name="NavigationStackPanel"
                    Grid.Row="1" />
        <Button Grid.Row="2"
                Command="{Binding DataContext.BackToMainWindowNavigationButtonsViewButtonCommand, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Window}}}"
                Content="{x:Static res:General.Back}"
                Style="{StaticResource InstallationAppNavigationButtonStyle}" />
    </Grid>
</UserControl>
