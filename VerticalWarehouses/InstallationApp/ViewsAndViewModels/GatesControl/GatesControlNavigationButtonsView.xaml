<UserControl x:Class="Ferretto.VW.InstallationApp.ViewsAndViewModels.GatesControl.GatesControlNavigationButtonsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Ferretto.VW.InstallationApp.ViewsAndViewModels.GatesControl"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:res="clr-namespace:Ferretto.Common.Resources;assembly=Ferretto.Common.Resources"
             mc:Ignorable="d">
    <DockPanel>
        <StackPanel DockPanel.Dock="Top">
            <Button />
        </StackPanel>
        <Button Content="{x:Static res:General.Back}"
                DockPanel.Dock="Bottom"
                Style="{StaticResource InstallationAppNavigationButtonStyle}" />
    </DockPanel>
</UserControl>
