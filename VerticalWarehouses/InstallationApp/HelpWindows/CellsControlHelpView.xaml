<UserControl x:Class="Ferretto.VW.InstallationApp.CellsControlHelpView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Ferretto.VW.InstallationApp"
             xmlns:res="clr-namespace:Ferretto.VW.Resources;assembly=Ferretto.VW.Resources"
             mc:Ignorable="d">
    <Grid Width="1024"
          Height="688"
          Background="{DynamicResource VWAPP_BACKGROUND}">
        <Image Grid.Row="0" Width="200"
                       Height="60"
                       Margin="10,10,0,618"
               HorizontalAlignment="Left"
                       Source="{DynamicResource Ferretto_Logo}"
                       Stretch="Uniform" />
        <TextBlock Grid.Row="0"
                   Text="{x:Static res:InstallationApp.CellsControl}"
                   Style="{StaticResource VWAPP_ViewTitleTextBlockStyle}" Margin="248,24,0,628"></TextBlock>
    </Grid>
</UserControl>
