        Title="MainWindow"
        Width="800"
        Height="450"
        mc:Ignorable="d">
    <Grid>
        <Button Margin="263,95,267,240"
                Command="{Binding ConnectToServiceCommand}">
            Connect
        </Button>
        <Button Margin="263,241,267,94"
                Command="{Binding ExecuteServerMethodCommand}">
            Execute method
        </Button>
    </Grid>
</Window>
