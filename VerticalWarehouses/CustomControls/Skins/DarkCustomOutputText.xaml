<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Ferretto.VW.CustomControls.Skins"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <SolidColorBrush x:Key="VWAPP_CustomOutputText_Foreground_Normal"
                     Color="{DynamicResource ColorFerrettoPureWhite}" />
    <sys:Double x:Key="VWAPP_CustomOutputText_FontSize_Label">10</sys:Double>
    <FontFamily x:Key="VWAPP_CustomOutputText_FontFamily">pack://application:,,,/Fonts/#HelveticaNeue-LightCond.otf</FontFamily>
    <sys:Double x:Key="VWAPP_CustomOutputText_FontSize_Output">14</sys:Double>

</ResourceDictionary>
