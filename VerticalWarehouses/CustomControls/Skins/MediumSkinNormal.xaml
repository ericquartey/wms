<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Ferretto.VW.CustomControls.Skins">

    <SolidColorBrush x:Key="IAPP_NavigationButton_Background_Normal"
                     Color="{DynamicResource EDBGrayF}" />
    <SolidColorBrush x:Key="IAPP_NavigationButton_Foreground_Normal"
                     Color="{DynamicResource ColorFerrettoPureWhite}" />
    <SolidColorBrush x:Key="IAPP_NavigationButton_BorderBrush_Normal"
                     Color="{DynamicResource ColorFerrettoLightGray}" />

    <SolidColorBrush x:Key="IAPP_ViewButton_Background_Normal"
                     Color="Transparent" />
    <SolidColorBrush x:Key="IAPP_ViewButton_Foreground_Normal"
                     Color="#fafafa" />
    <SolidColorBrush x:Key="IAPP_ViewButton_BorderBrush_Normal"
                     Color="#505050" />

    <SolidColorBrush x:Key="VWAPP_ViewMachineStateComboBox_Background_Normal"
                     Color="{DynamicResource ColorFerrettoMediumGray}" />
    <SolidColorBrush x:Key="VWAPP_ViewMachineStateComboBox_BorderBrush_Normal"
                     Color="{DynamicResource ColorFerrettoMediumGray}" />

    <SolidColorBrush x:Key="VWAPP_MainWindowMachineStateComboBox_Background_Normal"
                     Color="{DynamicResource ColorFerrettoDarkGray}" />
    <SolidColorBrush x:Key="VWAPP_MainWindowMachineStateComboBox_Foreground_Normal"
                     Color="{DynamicResource ColorFerrettoPureWhite}" />
    <SolidColorBrush x:Key="VWAPP_MainWindowMachineStateComboBox_BorderBrush_Normal"
                     Color="{DynamicResource ColorFerrettoDarkGray}" />

    <SolidColorBrush x:Key="VWAPP_MainWindowUserComboBox_Background_Normal"
                     Color="{DynamicResource ColorFerrettoBlack}" />
    <SolidColorBrush x:Key="VWAPP_MainWindowUserComboBox_Foreground_Normal"
                     Color="{DynamicResource ColorFerrettoPureWhite}" />
    <SolidColorBrush x:Key="VWAPP_MainWindowUserComboBox_BorderBrush_Normal"
                     Color="{DynamicResource ColorFerrettoBlack}" />

    <SolidColorBrush x:Key="VWAPP_ErrorTextBlock_Foreground_Normal"
                     Color="{DynamicResource ColorFerrettoRed}" />
    <SolidColorBrush x:Key="VWAPP_ViewBodyTextBlock_Foreground_Normal"
                     Color="{DynamicResource ColorFerrettoPureWhite}" />
    <SolidColorBrush x:Key="VWAPP_CCLabelTextBlock_Foreground_Normal"
                     Color="{DynamicResource ColorFerrettoPureWhite}" />
    <SolidColorBrush x:Key="VWAPP_SSTitleViewTextBlock_Foreground_Normal"
                     Color="{DynamicResource ColorFerrettoPureWhite}" />
    <SolidColorBrush x:Key="VWAPP_SSSubtitleTextBlock_Foreground_Normal"
                     Color="{DynamicResource ColorFerrettoPureWhite}" />
    <SolidColorBrush x:Key="VWAPP_ViewTitleViewTextBlock_Foreground_Normal"
                     Color="{DynamicResource ColorFerrettoPureWhite}" />

    <SolidColorBrush x:Key="VWAPP_CCInput_Foreground_Normal"
                     Color="{DynamicResource ColorFerrettoPureWhite}" />
    <SolidColorBrush x:Key="VWAPP_CCInput_Highlighted_Normal"
                     Color="{DynamicResource ColorFerrettoRed}" />
    <SolidColorBrush x:Key="VWAPP_CCInput_NotHighlighted_Normal"
                     Color="{DynamicResource ColorFerrettoLightGray}" />

    <SolidColorBrush x:Key="VWAPP_SensorState_Foreground_Normal"
                     Color="{DynamicResource ColorFerrettoPureWhite}" />
    <SolidColorBrush x:Key="VWAPP_SensorState_BulletBackground_Normal"
                     Color="{DynamicResource ColorFerrettoPureWhite}" />

</ResourceDictionary>
