<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Ferretto.VW.CustomControls.Skins"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:Double x:Key="VWAPP_CustomControlSensor_TextBlock_FontSize">14</sys:Double>
    <FontFamily x:Key="VWAPP_CustomControlSensor_TextBlock_FontFamily">pack://application:,,,/Fonts/#HelveticaNeue-LightCond.otf</FontFamily>

    <SolidColorBrush x:Key="VWAPP_CustomControlSensor_BulletBrush_Up"
                     Color="{DynamicResource ColorFerrettoGreen}" />
    <SolidColorBrush x:Key="VWAPP_CustomControlSensor_BulletBrush_Down"
                     Color="{DynamicResource ColorFerrettoMediumGray}" />
    <SolidColorBrush x:Key="VWAPP_CustomControlSensor_TextBlock_Foreground_Normal"
                     Color="{DynamicResource ColorFerrettoPureWhite}" />

</ResourceDictionary>
