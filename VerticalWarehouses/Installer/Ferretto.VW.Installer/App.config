<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    
    <add key="MAS:DirName" value="automation_service"/>
    <add key="MAS:FileName" value="Ferretto.VW.MAS.AutomationService.exe" />
    <add key="MAS:BaseUrl" value="http://192.168.0.10:5000"/>
    <add key="MAS:Service:Name" value="Ferretto_VW_MAS"/>
    <add key="MAS:Service:DisplayName" value="Ferretto automation service"/>
    <add key="MAS:Service:Description" value="Vertimag machine automation service"/>
    
    <add key="TS:DirName" value="telemetry_service"/>
    <add key="TS:FileName" value="Ferretto.VW.TelemetryService.exe" />
    <add key="TS:Database:Root" value="F:\Database"/>
    <add key="TS:DatabaseFolder" value="Telemetry"/>
    <add key="TS:BaseUrl" value="http://192.168.0.10:5060"/>
    <add key="TS:Service:Name" value="Ferretto_VW_TS"/>
    <add key="TS:Service:DisplayName" value="Ferretto telemetry service"/>
    <add key="TS:Service:Description" value="Vertimag machine telemetry service"/>
    
    <add key="PPC:DirName" value="panelpc_app"/>
    <add key="PPC:FileName" value="Ferretto.VW.App.exe"/>
