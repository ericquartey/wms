    <add key="Install:Bay1:Ipaddress" value="192.168.0.10"/>
    <add key="Install:Bay2:Ipaddress" value="192.168.0.20"/>
    <add key="Install:Bay3:Ipaddress" value="192.168.0.30"/>
    <add key="Install:Parameter:PpcIpAddress" value="**Set_at_Runtime**" />

    <add key="Backup:Root:Path" value="F:\Update\Backup"/> 
    <add key="Update:Temp:Path" value="F:\Update\Temp"/>

