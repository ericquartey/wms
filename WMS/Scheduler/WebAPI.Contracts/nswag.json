{
  "runtime": "NetCore21",
  "defaultVariables": null,
  "swaggerGenerator": {
    "aspNetCoreToSwagger": {
      "configuration": "$(Configuration)",
      "project": "../Scheduler.WebAPI/Ferretto.WMS.Scheduler.WebAPI.csproj"

      //...
    }
  },
  "codeGenerators": {
    "swaggerToCSharpClient": {
      "clientBaseClass": "ServiceBase",
      "className": "{controller}Service",
      "configurationClass": null,
      "generateClientClasses": true,
      "generateClientInterfaces": true,

      "useHttpRequestMessageCreationMethod": true,
      "clientClassAccessModifier": "public",
      "typeAccessModifier": "public",
      "generateContractsOutput": true,
      "contractsNamespace": "Ferretto.WMS.Scheduler.WebAPI.Contracts",
      "contractsOutputFilePath": "ServiceContracts.autogen.cs",
      "namespace": "Ferretto.WMS.Scheduler.WebAPI.Contracts",
      "output": "Services.autogen.cs"
    }
  }
}
