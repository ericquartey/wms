<wms:WmsView x:Class="Ferretto.WMS.App.Modules.MasterData.LoadingUnitDetailsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:ctrls="clr-namespace:Ferretto.Common.Controls.WPF;assembly=Ferretto.Common.Controls.WPF"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
             xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core"
             xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:modules="clr-namespace:Ferretto.Common.Utils.Modules;assembly=Ferretto.Common.Utils"
             xmlns:res="clr-namespace:Ferretto.WMS.App.Resources;assembly=Ferretto.WMS.App.Resources"
             xmlns:wms="clr-namespace:Ferretto.WMS.App.Controls;assembly=Ferretto.WMS.App.Controls"
             xmlns:enums="clr-namespace:Ferretto.Common.Resources.Enums;assembly=Ferretto.Common.Resources"
             Title="{x:Static res:Navigation.LoadingUnitDetails}"
             d:DesignHeight="450"
             d:DesignWidth="800"
             EnableHistoryView="True"
             SubTitle="{Binding Model.Code, StringFormat={x:Static res:Title.LoadingUnitDetailsView}}"
             SubTitleVisibility="{Binding Model, Converter={StaticResource RTVC}}"
             mc:Ignorable="d">
    <wms:WmsView.Resources>
        <wms:StringFormatConverter x:Key="SFC" />

        <DataTemplate x:Key="CellDisplayTemplateStock">
            <TextBlock Style="{DynamicResource TextBlockCell}">
                <TextBlock.Text>
                    <MultiBinding Converter="{StaticResource SFC}">
                        <Binding Source="{x:Static res:General.QuantityWithUnitMeasure}" />
                        <Binding Path="RowData.Row.Stock"
                                 TargetNullValue="" />
                        <Binding Path="RowData.Row.ItemMeasureUnit"
                                 TargetNullValue="" />
                    </MultiBinding>
                </TextBlock.Text>
                <TextBlock.ToolTip>
                    <MultiBinding Converter="{StaticResource SFC}">
                        <Binding Source="{x:Static res:General.QuantityWithUnitMeasure}" />
                        <Binding Path="RowData.Row.Stock"
                                 TargetNullValue="" />
                        <Binding Path="RowData.Row.ItemMeasureUnit"
                                 TargetNullValue="" />
                    </MultiBinding>
                </TextBlock.ToolTip>
            </TextBlock>
        </DataTemplate>

        <DataTemplate x:Key="CellDisplayTemplateMaxCapacity">
            <TextBlock Style="{DynamicResource TextBlockCell}">
                <TextBlock.Text>
                    <MultiBinding Converter="{StaticResource SFC}">
                        <Binding Source="{x:Static res:General.QuantityWithUnitMeasure}" />
                        <Binding Path="RowData.Row.MaxCapacity"
                                 TargetNullValue="" />
                        <Binding Path="RowData.Row.ItemMeasureUnit"
                                 TargetNullValue="" />
                    </MultiBinding>
                </TextBlock.Text>
                <TextBlock.ToolTip>
                    <MultiBinding Converter="{StaticResource SFC}">
                        <Binding Source="{x:Static res:General.QuantityWithUnitMeasure}" />
                        <Binding Path="RowData.Row.MaxCapacity"
                                 TargetNullValue="" />
                        <Binding Path="RowData.Row.ItemMeasureUnit"
                                 TargetNullValue="" />
                    </MultiBinding>
                </TextBlock.ToolTip>
            </TextBlock>
        </DataTemplate>
    </wms:WmsView.Resources>

    <Grid HorizontalAlignment="Stretch"
          VerticalAlignment="Stretch"
          IsEnabled="{Binding IsBusy, Converter={StaticResource BNC}}">

        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="Auto"
                              MaxWidth="400" />
        </Grid.ColumnDefinitions>

        <dxt:WaitIndicator Grid.Row="1"
                           VerticalAlignment="Center"
                           Panel.ZIndex="1"
                           Content="{x:Static res:DesktopApp.Loading}"
                           DeferedVisibility="{Binding IsBusy}" />

        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>

            <wms:NotifyControl SubTitleDimension="{Binding SubTitleDimension, RelativeSource={RelativeSource AncestorType={x:Type wms:WmsHistoryView}}, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" />

            <wms:ActionBar Grid.Column="1"
                           MinWidth="100"
                           Margin="0,10,10,0"
                           HorizontalAlignment="Right"
                           Style="{DynamicResource ActionBarNotCustomizable}">

                <wms:ActionBarItem Command="{Binding EditLoadingUnitCommand}"
                                   Content="{x:Static res:General.Edit}"
                                   CustomGlyphSize="24,24"
                                   Glyph="pack://application:,,,/Ferretto.WMS.App.Themes;component/icons/material/editor/1x_web/ic_mode_edit_black_24dp.png"
                                   GlyphSize="Custom"
                                   IsVisible="{Binding Model.LoadingUnitTypeHasCompartments}" />

                <wms:ActionBarItem Command="{Binding SaveCommand}"
                                   Content="{x:Static res:General.Save}"
                                   CustomGlyphSize="24,24"
                                   Glyph="pack://application:,,,/Ferretto.WMS.App.Themes;component/icons/material/content/1x_web/ic_save_black_24dp.png"
                                   GlyphSize="Custom"
                                   TooltipText="{Binding SaveReason}" />
                <wms:ActionBarItem Command="{Binding RevertCommand}"
                                   Content="{x:Static res:General.Revert}"
                                   CustomGlyphSize="24,24"
                                   Glyph="pack://application:,,,/Ferretto.WMS.App.Themes;component/icons/material/content/1x_web/ic_undo_black_24dp.png"
                                   GlyphSize="Custom" />

                <wms:ActionBarItem Command="{Binding WithdrawLoadingUnitCommand}"
                                   Content="{x:Static res:MasterData.Withdraw}"
                                   CustomGlyphSize="24,24"
                                   Glyph="pack://application:,,,/Ferretto.WMS.App.Themes;component/icons/material/av/1x_web/ic_play_arrow_black_24dp.png"
                                   GlyphSize="Custom"
                                   TooltipText="{Binding WithdrawReason}" />

                <wms:ActionBarItem Command="{Binding DeleteCommand}"
                                   Content="{x:Static res:General.Delete}"
                                   CustomGlyphSize="24,24"
                                   Glyph="pack://application:,,,/Ferretto.WMS.App.Themes;component/icons/material/action/1x_web/ic_delete_black_24dp.png"
                                   GlyphSize="Custom" />

                <wms:ActionBarItem Command="{Binding RefreshCommand}"
                                   Content="{x:Static res:General.Refresh}"
                                   CustomGlyphSize="24,24"
                                   Glyph="pack://application:,,,/Ferretto.WMS.App.Themes;component/icons/material/navigation/1x_web/ic_refresh_black_24dp.png"
                                   GlyphSize="Custom" />
            </wms:ActionBar>

            <StackPanel Orientation="Vertical"
                        Grid.Row="1"
                        Grid.ColumnSpan="2"
                        Margin="0,0,10,5"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Top">
                <TextBlock Style="{DynamicResource TextBlockErrorMessageStyle}"
                           Text="{Binding Model.Error}"
                           TextAlignment="Right"
                           Visibility="{Binding Model.Error, Converter={StaticResource STVC}}" />
                <TextBlock Style="{DynamicResource TextBlockErrorMessageStyle}"
                           Text="{x:Static res:Errors.ThisObjectHasUnsavedChangesButDataNeedToBeRefreshed}"
                           TextAlignment="Right">
                    <TextBlock.Visibility>
                        <MultiBinding Converter="{StaticResource MBATVC}">
                            <Binding Path="HasConflictingChanges" />
                            <Binding Path="IsBusy" Converter="{StaticResource BNC}" />
                        </MultiBinding>
                    </TextBlock.Visibility>
                </TextBlock>
            </StackPanel>

        </Grid>

        <ScrollViewer Grid.Row="1"
                      Margin="10,0,10,10"
                      Focusable="False"
                      IsTabStop="True"
                      Visibility="{Binding Model, Converter={StaticResource RTVC}}">
            <Grid Margin="0,0,5,0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>

                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>

                <GroupBox Grid.Row="0"
                          Grid.RowSpan="2"
                          Grid.Column="0"
                          Margin="0"
                          Header="{x:Static res:MasterData.LoadingUnitInformation}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <wms:TextBox Grid.ColumnSpan="3"
                                     Margin="0,0,20,0"
                                     wms:ShowTitle.Show="True"
                                     EditValue="{Binding Model.Code, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" />
                        <wms:ComboBox Grid.Row="1"
                                      Grid.ColumnSpan="3"
                                      Margin="0,0,20,0"
                                      wms:ShowTitle.Show="True"
                                      EditValue="{Binding Model.LoadingUnitTypeId, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                      ItemsSource="{Binding Model.LoadingUnitTypeChoices}" />
                        <wms:ComboBox Grid.Row="2"
                                      Margin="0,0,20,0"
                                      wms:ShowTitle.Show="True"
                                      EditValue="{Binding Model.LoadingUnitStatusId, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                      ItemsSource="{Binding Model.LoadingUnitStatusChoices}" />
                        <wms:ComboBox Grid.Row="2"
                                      Grid.Column="1"
                                      Margin="0,0,20,0"
                                      wms:ShowTitle.Show="True"
                                      EditValue="{Binding Model.AbcClassId, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                      ItemsSource="{Binding Model.AbcClassChoices}" />
                        <wms:ComboBox Grid.Row="2"
                                      Grid.Column="2"
                                      Margin="0,0,20,0"
                                      wms:ShowTitle.Show="True"
                                      EditValue="{Binding Model.ReferenceType, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                      ItemTemplate="{DynamicResource ComboBoxEditTranslation}">
                            <dxmvvm:Interaction.Behaviors>
                                <dxmvvm:EnumItemsSourceBehavior EnumType="{x:Type enums:ReferenceType}"
                                                                SortMode="DisplayName" />
                            </dxmvvm:Interaction.Behaviors>
                        </wms:ComboBox>

                        <wms:TextBox Grid.Row="3"
                                     Grid.ColumnSpan="3"
                                     Height="100"
                                     Margin="0,0,20,0"
                                     wms:ShowTitle.Show="True"
                                     EditValue="{Binding Model.Note, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                     IsMultiline="True" />
                    </Grid>
                </GroupBox>

                <GroupBox Grid.Row="0"
                          Grid.Column="1"
                          Margin="0"
                          Header="{x:Static res:MasterData.CurrentCellInformation}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <wms:ComboBox Grid.ColumnSpan="2"
                                      Margin="0,0,20,0"
                                      wms:ShowTitle.Show="True"
                                      EditValue="{Binding Path=Model.CellId, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                      ItemsSource="{Binding Path=Model.CellChoices}" />
                        <wms:ComboBox Grid.Row="1"
                                      Margin="0,0,20,0"
                                      wms:ShowTitle.Show="True"
                                      EditValue="{Binding Path=Model.CellPositionId, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                      ItemsSource="{Binding Model.CellPositionChoices}" />

                        <wms:WmsCheckEdit Grid.Row="1"
                                          Grid.Column="1"
                                          Margin="0,0,20,0"
                                          wms:ShowTitle.Show="True"
                                          Content="{x:Static res:BusinessObjects.PairingFixed}"
                                          IsChecked="{Binding Path=Model.IsCellPairingFixed, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" />

                    </Grid>
                </GroupBox>

                <GroupBox Grid.Row="1"
                          Grid.Column="1"
                          Margin="0"
                          Header="{x:Static res:MasterData.PhysicalInformation}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <wms:SpinEdit Margin="0,0,20,0"
                                      wms:ShowTitle.Show="True"
                                      EditValue="{Binding Path=Model.Height, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}"
                                      Style="{DynamicResource SpinEditDecimalStyle}" />
                        <wms:SpinEdit Grid.Column="1"
                                      Margin="0,0,20,0"
                                      wms:ShowTitle.Show="True"
                                      EditValue="{Binding Path=Model.Weight, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" />
                        <wms:SpinEdit Grid.Column="2"
                                      Margin="0,0,20,0"
                                      wms:ShowTitle.Show="True"
                                      EditValue="{Binding Path=Model.HandlingParametersCorrection, Mode=TwoWay, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" />

                    </Grid>
                </GroupBox>

                <GroupBox Grid.Row="2"
                          Grid.Column="0"
                          Grid.ColumnSpan="2"
                          MinHeight="190"
                          VerticalAlignment="Stretch"
                          Header="{x:Static res:MasterData.CompartmentsLayoutOverview}"
                          Visibility="{Binding Model.LoadingUnitTypeHasCompartments, Converter={StaticResource BTVC}}">
                    <ctrls:TrayControl HorizontalAlignment="Stretch"
                                       VerticalAlignment="Stretch"
                                       CommandDoubleClick="{Binding EditLoadingUnitCommand}"
                                       Compartments="{Binding Model.Compartments}"
                                       DimensionHeight="{Binding Model.Depth}"
                                       DimensionWidth="{Binding Model.Width}"
                                       IsCompartmentSelectable="{Binding IsCompartmentSelectableTray, Mode=OneWay}"
                                       RulerInfo="mm"
                                       SelectedColorFilterFunc="{Binding TrayColoringFunc, Mode=OneWay}"
                                       SelectedItem="{Binding SelectedCompartment, Mode=TwoWay}"
                                       ShowBackground="True"
                                       ShowLittleMark="False"
                                       ShowMark="True"
                                       ShowMiddleMark="False" />
                </GroupBox>

                <GroupBox Grid.Row="3"
                          Grid.Column="0"
                          Grid.ColumnSpan="2"
                          Margin="0"
                          Header="{x:Static res:MasterData.Compartments}"
                          Visibility="{Binding Model.LoadingUnitTypeHasCompartments, Converter={StaticResource BTVC}}">
                    <StackPanel Margin="0,5,0,0">
                        <TextBlock Text="{x:Static res:MasterData.LoadingUnitWithNoCompartment}"
                                   Visibility="{Binding LoadingUnitHasCompartments, Converter={StaticResource BTVIC}}" />
                        <wms:WmsGridControl MinHeight="200"
                                            MaxWidth="2000"
                                            MaxHeight="200"
                                            ItemsSource="{Binding CompartmentsDataSource, Mode=OneWay}"
                                            SelectedBusinessObject="{Binding SelectedCompartment, Mode=TwoWay}"
                                            Visibility="{Binding LoadingUnitHasCompartments, Converter={StaticResource BTVC}}">
                            <wms:WmsGridControl.View>
                                <dxg:TableView>
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="RowDoubleClick">
                                            <wms:WmsHistoryViewAppear Id="{Binding SelectedCompartment.Id, Mode=OneWay}"
                                                                      StartModuleName="{wms:NameOf Type={x:Type modules:MasterData}}"
                                                                      StartViewName="{x:Static modules:MasterData.COMPARTMENTDETAILS}" />
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                </dxg:TableView>
                            </wms:WmsGridControl.View>

                            <wms:WmsGridControl.Columns>
                                <dxg:GridColumn Width="130*"
                                                Header="{x:Static res:General.Position}">
                                    <dxg:GridColumn.CellDisplayTemplate>
                                        <DataTemplate>
                                            <TextBlock Style="{DynamicResource TextBlockCell}">
                                                <TextBlock.Text>
                                                    <MultiBinding Converter="{StaticResource SFC}">
                                                        <Binding Source="{x:Static res:General.PositionFormat}" />
                                                        <Binding Path="Data.XPosition"
                                                                 TargetNullValue="" />
                                                        <Binding Path="Data.YPosition"
                                                                 TargetNullValue="" />
                                                    </MultiBinding>
                                                </TextBlock.Text>
                                                <TextBlock.ToolTip>
                                                    <MultiBinding Converter="{StaticResource SFC}">
                                                        <Binding Source="{x:Static res:General.PositionFormat}" />
                                                        <Binding Path="Data.XPosition"
                                                                 TargetNullValue="" />
                                                        <Binding Path="Data.YPosition"
                                                                 TargetNullValue="" />
                                                    </MultiBinding>
                                                </TextBlock.ToolTip>
                                            </TextBlock>
                                        </DataTemplate>
                                    </dxg:GridColumn.CellDisplayTemplate>
                                </dxg:GridColumn>
                                <dxg:GridColumn Width="110*"
                                                Header="{x:Static res:General.Dimension}">
                                    <dxg:GridColumn.CellDisplayTemplate>
                                        <DataTemplate>
                                            <TextBlock Style="{DynamicResource TextBlockCell}">
                                                <TextBlock.Text>
                                                    <MultiBinding Converter="{StaticResource SFC}">
                                                        <Binding Source="{x:Static res:General.DimensionFormat}" />
                                                        <Binding Path="Data.Width"
                                                                 TargetNullValue="" />
                                                        <Binding Path="Data.Depth"
                                                                 TargetNullValue="" />
                                                    </MultiBinding>
                                                </TextBlock.Text>
                                                <TextBlock.ToolTip>
                                                    <MultiBinding Converter="{StaticResource SFC}">
                                                        <Binding Source="{x:Static res:General.DimensionFormat}" />
                                                        <Binding Path="Data.Width"
                                                                 TargetNullValue="" />
                                                        <Binding Path="Data.Depth"
                                                                 TargetNullValue="" />
                                                    </MultiBinding>
                                                </TextBlock.ToolTip>
                                            </TextBlock>
                                        </DataTemplate>
                                    </dxg:GridColumn.CellDisplayTemplate>
                                </dxg:GridColumn>
                                <dxg:GridColumn Width="100*"
                                                FieldName="ItemCode" />
                                <dxg:GridColumn Width="280*"
                                                FieldName="ItemDescription" />
                                <dxg:GridColumn Width="90*"
                                                FieldName="Sub1" />
                                <dxg:GridColumn Width="90*"
                                                FieldName="Sub2" />
                                <dxg:GridColumn Width="180*"
                                                FieldName="Lot" />
                                <dxg:GridColumn Width="100*"
                                                FieldName="CompartmentStatusDescription" />
                                <dxg:GridColumn Width="125*"
                                                CellDisplayTemplate="{StaticResource CellDisplayTemplateStock}"
                                                FieldName="Stock" />
                                <dxg:GridColumn Width="125*"
                                                CellDisplayTemplate="{StaticResource CellDisplayTemplateMaxCapacity}"
                                                FieldName="MaxCapacity" />
                                <dxg:GridColumn Width="90*"
                                                CellDisplayTemplate="{DynamicResource BooleanCellTemplate}"
                                                FieldName="IsItemPairingFixed" />
                            </wms:WmsGridControl.Columns>

                            <wms:WmsGridControl.SortInfo>
                                <dxg:GridSortInfo FieldName="LoadingUnitCode"
                                                  SortOrder="Ascending" />
                            </wms:WmsGridControl.SortInfo>

                        </wms:WmsGridControl>

                    </StackPanel>

                </GroupBox>

            </Grid>
        </ScrollViewer>

        <ScrollViewer Grid.RowSpan="2"
                      Grid.Column="1"
                      Focusable="False"
                      IsTabStop="False"
                      Visibility="{Binding Model, Converter={StaticResource RTVC}}">

            <Border Style="{StaticResource InfoBoxBorderStyle}"
                    Visibility="{Binding ActualWidth, RelativeSource={RelativeSource AncestorType={x:Type wms:WmsView}}, Converter={StaticResource BTTVC}, ConverterParameter={StaticResource MinWidthDetailsView}}">
                <GroupBox Margin="20,0"
                          Header="{x:Static res:General.OtherInformation}">
                    <StackPanel Orientation="Vertical">
                        <wms:InfoText HorizontalAlignment="Left"
                                      wms:ShowTitle.Show="True"
                                      Content="{Binding Model.CreationDate}" />
                        <wms:InfoText HorizontalAlignment="Left"
                                      wms:ShowTitle.Show="True"
                                      Content="{Binding Model.LastHandlingDate}" />
                        <wms:InfoText HorizontalAlignment="Left"
                                      wms:ShowTitle.Show="True"
                                      Content="{Binding Model.InventoryDate}" />
                        <wms:InfoText HorizontalAlignment="Left"
                                      wms:ShowTitle.Show="True"
                                      Content="{Binding Model.LastPickDate}" />
                        <wms:InfoText HorizontalAlignment="Left"
                                      wms:ShowTitle.Show="True"
                                      Content="{Binding Model.LastPutDate}" />
                        <wms:InfoText HorizontalAlignment="Left"
                                      wms:ShowTitle.Show="True"
                                      Content="{Binding Model.MissionsCount}" />
                    </StackPanel>
                </GroupBox>

            </Border>

        </ScrollViewer>

    </Grid>
</wms:WmsView>
