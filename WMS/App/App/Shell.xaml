<wms:WmsWindow x:Class="Ferretto.WMS.App.Shell"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:dxcore="http://schemas.devexpress.com/winfx/2008/xaml/core"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:prism="http://www.codeplex.com/prism"
                 xmlns:wms="clr-namespace:Ferretto.WMS.App"
                 xmlns:r="clr-namespace:Ferretto.Common.Resources;assembly=Ferretto.Common.Resources"
                 Title="{x:Static r:DesktopApp.Application_Title}"
                 InitialHeight="950"
                 InitialWidth="1440"
                 MarginWidth="50"
                 MarginHeight="50"
                 MinWidth="1024"
                 MinHeight="576"
                 WindowStartupLocation="CenterScreen"  
                 mc:Ignorable="d">
    <Grid>
        <ContentControl x:Name="MainContent"
                        prism:RegionManager.RegionName="Layout.MainContent" />
    </Grid>
</wms:WmsWindow>
